const hosts = JSON.parse('{{ dns_config.domains | to_json }}');

interface Provider {
    wallet_address?: `0x${string}`
    supportedChains?: number[]
    accountIndex?: number
    privateKey?: `0x${string}`
    mnemonic?: string,
    a_record: string[],
    cname: string
}

export interface Env {
    MARKETPLACE_ID: number;
    PURGE_PASSWORD: string;
    dbName: string,
    dbUser: string,
    dbPassword: string,
    dbHost: string,
    dbPort: number,
    dbDialect: "postgres",
    caddyUrl: string;
    hosts: string[];
    hns: string;
    providers: Provider[];
    debug?: boolean
    elasticSearchUrl: string
}

